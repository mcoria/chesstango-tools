<configuration>
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>


    <logger name="net.chesstango.uci.proxy" level="INFO"/>


    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %-25.25logger{25} - %msg%n</pattern>
		</encoder>
	</appender>

</configuration>
